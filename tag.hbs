{{!< default }}

{{#tag}}
    <section class="cover {{#if feature_image}}isCoverEnable"
        style="background-image: url({{feature_image}}){{else}}isCoverDisabled{{/if}}">

        {{> "cover/cover-filtered" }}

    </section>
{{/tag}}

<main id="site-main" class="site-main" role="main">
    <section class="site-main-container u-content-center">
        <div class="site-main-wrapper">
            {{> "posts/post-card-filtered"}}
        </div>
    </section>
</main>

{{#if @labs.members}}
    <section class="subscription">
        {{> subscription/subscription }}
    </section>
{{/if}}